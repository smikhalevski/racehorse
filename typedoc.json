{
  "name": "Racehorse",
  "entryPoints": ["./web/racehorse", "./web/react"],
  "excludePrivate": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "entryPointStrategy": "packages",
  "disableSources": true,
  "hideGenerator": true,
  "readme": "none",
  "tsconfig": "./tsconfig.json",
  "navigation": {
    "includeFolders": false
  },
  "sidebarLinks": {
    "Racehorse Android API": "/racehorse/android"
  },
  "plugin": ["typedoc-plugin-mdn-links"],
  "externalSymbolLinkMappings": {
    "parallel-universe": {
      "AbortableCallback": "https://smikhalevski.github.io/parallel-universe/types/AbortableCallback.html",
      "AbortablePromise": "https://smikhalevski.github.io/parallel-universe/classes/AbortablePromise.html"
    }
  }
}
