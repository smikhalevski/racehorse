    "declaration": true,
    "isolatedModules": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "NodeNext",
    "jsx": "react"
  },
  "include": ["./src/**/*"]
}
